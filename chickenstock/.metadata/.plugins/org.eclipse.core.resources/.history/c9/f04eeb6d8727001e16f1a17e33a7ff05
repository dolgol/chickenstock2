	public static void main(String[] args) {
		String KEY_WORD = "107600";
		
		getPA(KEY_WORD, 2);
	}
	/**
	* URL 완성
	*@Param KEY_WORD
	*@Param PAGE
	*@return
	*/
	public static String getParameter(String KEY_WORD, int PAGE) {
		String params = "?code=" + KEY_WORD + "" 
								 + "itemTotalCnt:3"
								 + "item_list:[,...]"
								 + "login:'true'"
								 + "next_page: 0"
								 + "page: 1"
								 + "prev_page:0"
								 + "sel_cid:null"
								 + "type:'recent'"
		return "";
	}
}
