package ssg.com.a.dao;

import java.util.List;

import ssg.com.a.dto.stockDto;

public class stockDao {

	
	public interface stockregi {
		void save();
		
		List<stockDto> findNumber(int number);
		List<stockDto> findkos(String kos);
		List<stockDto> findname(String name);
		List<stockDto> findup(String up);
		List<stockDto> findupup(String upup);
		List<stockDto> findcreateDay(String createDay);
		List<stockDto> findpresident(String president);
		List<stockDto> findhttp(String http);
		List<stockDto> findarea(String area);
		List<stockDto> findAll();
		
		
	}
}
