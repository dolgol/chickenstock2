package util;

public class PdsUtil {

	public static String getNewfileName(String filename) {
		String newfilename="";
		String fpost = "";	// .jpg, .txt ...
		
		if(filename.indexOf('.') >= 0) {	//확장자명이 있음
			fpost = filename.substring(filename.indexOf('.'));
			
		}
		else {	// -1
			
		}
		
	}
}
